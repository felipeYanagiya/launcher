<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="fyanagiya">
        <createTable tableName="survey">
            <column name="ID" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="PK_SURVEY" unique="true" nullable="false"/>
            </column>
            <column name="EXPECTATION" type="varchar(255)"/>
            <column name="PAY_FOR" type="varchar(255)"/>
            <column name="NEED" type="varchar(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
